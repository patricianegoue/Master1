#ifndef LISTECHAINE_H_
#define LISTECHAINE_H

struct Noeud{
	int info;
	struct Noeud * suivant;
};
typedef struct Noeud Noeud;

Noeud* nouveau(int val);
void insert_tete(Noeud** liste, int val);
void supprime_tete(Noeud ** liste);
void insert_queue(Noeud** liste_adr, int val, Noeud* liste_val);
void affiche(Noeud * liste);





#endif /* LISTECHAINE_H_ */
